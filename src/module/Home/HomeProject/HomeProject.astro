---
import HomeProjectComponent from '.';

import { getAllByType } from '@/js/core/prismic';
const allProjects= await getAllByType("project");

const {title, list} = Astro.props;

const Project = list.map((item) => {
    return allProjects.find((el) => item.project.id === el.id);
});
---

<HomeProjectComponent
    Title={title[0]}
    Project={Project}
    client:visible={{ rootMargin: "100% 0px 100% 0px" }}
/>