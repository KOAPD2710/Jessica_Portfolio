---
import ProjectDetailBodyComponent from './'
import * as prismic from '@prismicio/client'
const contentText = prismic.asHTML(Astro.props.data.content);
---


<ProjectDetailBodyComponent 
    {...Astro.props}
    client:visible={{ rootMargin: "100% 0px 100% 0px" }}
>
    <Fragment set:html={contentText} slot='bodyContent'/>
</ProjectDetailBodyComponent>
